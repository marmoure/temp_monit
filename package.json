{
  "name": "tempshit",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn --transpileOnly src/index.ts",
    "prisma": "prisma",
    "migrate": "prisma migrate dev",
    "clean": "rm -rf dist",
    "copy": "ncp src/public dist/public",
    "build": "npm run clean && tsc && npm run copy"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/node": "^18.15.0",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.9",
    "ncp": "^2.0.0",
    "prisma": "^5.7.0"
  }
}
